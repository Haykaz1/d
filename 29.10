using System;

Person tom = new Person("Tom");
Console.WriteLine(tom.name);

class Person
{
    public readonly string name = "Undefined"; 
    public Person(string name)
    {
        this.name = name; 
    }
	readonly struct Persn
{
    public readonly string name;
    public Persn(string name)
    {
        this.name = name;
    }
}
	readonly struct Prson
{
    public readonly string Name { get; } 
    public int Age { get; } 
    public Prson(string name, int age)
    {
        Name = name;
        Age = age;
    }
}
}
